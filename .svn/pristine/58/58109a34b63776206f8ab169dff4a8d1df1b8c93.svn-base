<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.guanglumedia.api.dao.ExtboothRestDao">
    <select id="getAllExtbooth" resultType="hashmap">
        select id,sort,type,objectid,Identify,imgurl from ext_booth where status=1
    </select>
    <select id="findCount" resultType="int">
        select count(*) from ext_booth where status=1
    </select>
    
</mapper>

